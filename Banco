  `idMATERIA` INT NOT NULL,
  `NOTA` DOUBLE NOT NULL,
  `BIMESTRE` INT NOT NULL,
  PRIMARY KEY (`idNOTA`),
  INDEX `fk_NOTA_1_idx` (`idALUNO` ASC),
  INDEX `fk_NOTA_2_idx` (`idMATERIA` ASC),
  CONSTRAINT `fk_NOTA_1`
