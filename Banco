  `QTD_AULAS` INT NOT NULL,
  PRIMARY KEY (`idMATERIA`))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`TURMA_has_MATERIA`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`TURMA_has_MATERIA` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`TURMA_has_MATERIA` (
  `TURMA_idTURMA` INT NOT NULL,
  `MATERIA_idMATERIA` INT NOT NULL,
  `ANO` INT NOT NULL,
  PRIMARY KEY (`TURMA_idTURMA`, `MATERIA_idMATERIA`),
  INDEX `fk_TURMA_has_MATERIA_MATERIA1_idx` (`MATERIA_idMATERIA` ASC),
  INDEX `fk_TURMA_has_MATERIA_TURMA1_idx` (`TURMA_idTURMA` ASC),
  CONSTRAINT `fk_TURMA_has_MATERIA_TURMA1`
    FOREIGN KEY (`TURMA_idTURMA`)
    REFERENCES `LPTI`.`TURMA` (`idTURMA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_TURMA_has_MATERIA_MATERIA1`
    FOREIGN KEY (`MATERIA_idMATERIA`)
    REFERENCES `LPTI`.`MATERIA` (`idMATERIA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`FREQUENCIA`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`FREQUENCIA` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`FREQUENCIA` (
  `idFREQUENCIA` INT NOT NULL AUTO_INCREMENT,
  `idALUNO` INT NOT NULL,
  `idMATERIA` INT NOT NULL,
  `FALTAS` INT NOT NULL,
  `BIMESTRE` INT NOT NULL,
  PRIMARY KEY (`idFREQUENCIA`),
  INDEX `fk_FREQUENCIA_1_idx` (`idMATERIA` ASC),
  INDEX `fk_FREQUENCIA_2_idx` (`idALUNO` ASC),
  CONSTRAINT `fk_FREQUENCIA_1`
    FOREIGN KEY (`idMATERIA`)
    REFERENCES `LPTI`.`MATERIA` (`idMATERIA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_FREQUENCIA_2`
    FOREIGN KEY (`idALUNO`)
    REFERENCES `LPTI`.`ALUNO` (`idALUNO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`NOTA`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`NOTA` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`NOTA` (
