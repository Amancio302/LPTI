-- -----------------------------------------------------
-- Schema LPTI
-- -----------------------------------------------------
CREATE SCHEMA IF NOT EXISTS `LPTI` DEFAULT CHARACTER SET utf8 ;
USE `LPTI` ;

-- -----------------------------------------------------
-- Table `LPTI`.`USUARIO`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`USUARIO` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`USUARIO` (
  `idUSUARIO` INT NOT NULL AUTO_INCREMENT,
  `NOME` VARCHAR(60) NULL,
  `LOGIN` VARCHAR(45) NOT NULL,
  `SENHA` VARCHAR(45) NOT NULL,
  `TIPO` INT NOT NULL,
  PRIMARY KEY (`idUSUARIO`),
  UNIQUE INDEX `LOGIN_UNIQUE` (`LOGIN` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`ALUNO`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`ALUNO` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`ALUNO` (
  `idALUNO` INT NOT NULL AUTO_INCREMENT,
  `NOME` VARCHAR(60) NOT NULL,
  `SITUACAO` INT NULL,
  `FOTO` BLOB NULL,
  PRIMARY KEY (`idALUNO`))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`CURSO`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`CURSO` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`CURSO` (
  `idCURSO` INT NOT NULL AUTO_INCREMENT,
  `NOME` VARCHAR(45) NOT NULL,
  `MODALIDADE` TINYINT(1) NOT NULL,
  PRIMARY KEY (`idCURSO`))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`TURMA`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`TURMA` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`TURMA` (
  `idTURMA` INT NOT NULL AUTO_INCREMENT,
  `idCURSO` INT NOT NULL,
  `SERIE` INT NOT NULL,
  PRIMARY KEY (`idTURMA`),
  INDEX `fk_TURMA_1_idx` (`idCURSO` ASC),
  CONSTRAINT `fk_TURMA_1`
    FOREIGN KEY (`idCURSO`)
    REFERENCES `LPTI`.`CURSO` (`idCURSO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`TURMA_has_ALUNO`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`TURMA_has_ALUNO` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`TURMA_has_ALUNO` (
  `ALUNO_idALUNO` INT NOT NULL,
  `TURMA_idTURMA` INT NOT NULL,
  `ANO` INT NOT NULL,
  PRIMARY KEY (`ALUNO_idALUNO`, `TURMA_idTURMA`),
  INDEX `fk_ALUNO_has_TURMA_TURMA1_idx` (`TURMA_idTURMA` ASC),
  INDEX `fk_ALUNO_has_TURMA_ALUNO_idx` (`ALUNO_idALUNO` ASC),
  CONSTRAINT `fk_ALUNO_has_TURMA_ALUNO`
    FOREIGN KEY (`ALUNO_idALUNO`)
    REFERENCES `LPTI`.`ALUNO` (`idALUNO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_ALUNO_has_TURMA_TURMA1`
    FOREIGN KEY (`TURMA_idTURMA`)
    REFERENCES `LPTI`.`TURMA` (`idTURMA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `LPTI`.`MATERIA`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `LPTI`.`MATERIA` ;

CREATE TABLE IF NOT EXISTS `LPTI`.`MATERIA` (
